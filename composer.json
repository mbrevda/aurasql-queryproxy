{
    "name": "mbrevda/aurasql-queryproxy",
    "description": "Class to proxy Aura Sql and Query methods",
    "require": {
        "aura/sql": "2.*",
        "aura/sqlquery": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "license": "BSD",
    "authors": [
        {
            "name": "Moshe Brevda",
            "email": "mbrevda@gmail.com"
        }
	],
    "autoload": {
        "psr-4": {
            "Mbrevda\\Queryproxy\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "process-timeout": 30
    }
}
